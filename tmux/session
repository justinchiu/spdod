new-window -n "root"
send-keys "conda deactivate" Enter
send-keys "poetry shell" Enter
split-window -h -p 50
send-keys "conda deactivate" Enter
send-keys "poetry shell" Enter

new-window -n "spdop"
send-keys "conda deactivate" Enter
send-keys "cd spdop" Enter
split-window -h -p 50
send-keys "conda deactivate" Enter
send-keys "cd spdop" Enter

new-window -n "agent"
send-keys "conda deactivate" Enter
send-keys "cd spdop/agent" Enter
split-window -h -p 50
send-keys "conda deactivate" Enter
send-keys "cd spdop/agent" Enter
